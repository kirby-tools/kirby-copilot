{
  "name": "kirby-copilot",
  "type": "module",
  "version": "3.2.0",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "mprocs",
    "build": "kirbyup src/panel/index.ts",
    "build:assets": "rm -rf assets && tsdown --config src/assets/tsdown.config.ts",
    "build:playground": "PLAYGROUND=true pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "release": "pnpm run build && pnpm run build:assets && bumpp --all --execute \"npx zx scripts/pre-release.mjs\" && zx scripts/post-release.mjs"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.15",
    "@ai-sdk/google": "^3.0.10",
    "@ai-sdk/mistral": "^3.0.9",
    "@ai-sdk/openai": "^3.0.12",
    "@ai-sdk/provider": "^3.0.4",
    "@ai-sdk/provider-utils": "^4.0.8",
    "@antfu/eslint-config": "^7.0.1",
    "@kirby-tools/licensing": "^0.6.10",
    "@types/node": "^25.0.9",
    "ai": "^6.0.39",
    "autoprefixer": "^10.4.23",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "kirby-types": "^1.4.5",
    "kirbyup": "^3.4.1",
    "kirbyuse": "^1.2.12",
    "mprocs": "^0.8.2",
    "ohash": "^2.0.11",
    "pdfjs-dist": "^5.4.530",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prosemirror-model": "^1.25.4",
    "prosemirror-state": "^1.4.4",
    "prosemirror-view": "^1.41.5",
    "tailwindcss": "^3.4.19",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "utilful": "^2.5.0",
    "vite": "~6.3.7",
    "vitest": "^4.0.17",
    "vue": "^2.7.16",
    "zod": "^4.3.5",
    "zx": "^8.8.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
